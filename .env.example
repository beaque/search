# Serper API
SERPER_API_KEY=your_serper_api_key_here

# AWS Bedrock (uses default credential chain if not set)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

# Redis (AWS ElastiCache or local)
# For ElastiCache: redis://your-cluster.xxxxx.cache.amazonaws.com:6379
# For local: redis://localhost:6379
REDIS_URL=redis://localhost:6379

# Open PageRank API (optional, free tier: 100k lookups/month)
# Get key at: https://www.domcop.com/openpagerank/
OPEN_PAGERANK_API_KEY=your_openpagerank_key

# Stripe Configuration (for billing)
# Get keys at: https://dashboard.stripe.com/test/apikeys
STRIPE_SECRET_KEY=sk_test_xxxxx
STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx

# From Dashboard → Billing → Meters (click on the meter)
STRIPE_METER_ID=mtr_xxxxx

# From Dashboard → Products → Click each product → Copy price ID
STRIPE_PRICE_FREE=price_xxxxx
STRIPE_PRICE_STARTER=price_xxxxx
STRIPE_PRICE_PRO=price_xxxxx
